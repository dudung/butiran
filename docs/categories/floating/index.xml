&lt;script&gt;
 document.addEventListener("DOMContentLoaded", function () {
 

/************************************************************
 * 1. CSS: container must match canvas internal size (515×270)
 ************************************************************/
const css = `
 #cnt,
 #cnt-inner {
 position: relative !important;
 overflow: visible !important;
 }

 /* MUST match actual canvas attribute size (515×270) */
 #cnt-inner {
 width: 515px !important;
 height: 270px !important;
 position: relative !important;
 margin: 0 !important;
 padding: 0 !important;
 }

 #cnt-inner canvas {
 position: absolute !important;
 left: 0 !important;
 top: 0 !important;
 max-width: none !important;
 width: 515px !important;
 height: 270px !important;
 margin: 0 !important;
 padding: 0 !important;
 display: block !important;
 }
`;
let st = document.createElement("style");
st.appendChild(document.createTextNode(css));
document.head.appendChild(st);


/************************************************************
 * 2. Create container
 ************************************************************/
let cnt = document.getElementById("cnt");
let div = document.createElement("div");
div.id = "cnt-inner";
cnt.appendChild(div);

/* to make it better in a note */
cnt.style.margin = "2em 1em 4em 1em";


/************************************************************
 * 3. Create plot (CanvasDataPlot auto-draws)
 ************************************************************/
var scatterPlot = new CanvasDataPlot(
 d3.select("#cnt-inner"),
 [600, 350], // keep this! margins reduce to 515×270 internally
 {
 xAxisLabel: "IQ",
 yAxisLabel: "Test Score",
 markerLineWidth: 3,
 markerRadius: 5
 }
);


/************************************************************
 * 4. Add data (auto-draws)
 ************************************************************/
var data = [
 [64, 50],
 [90.5, 60],
 [95, 25],
 [101, 13],
 [110, 86],
 [118, 55]
];

scatterPlot.addDataSet("dataSet1", "Test Results", data, "steelblue");

